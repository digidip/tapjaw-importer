{
  "name": "tapjaw-importer",
  "version": "0.1.4",
  "description": "Create your own stream of payload messages to STDOUT from any API.",
  "repository": "https://github.com/digidip/tapjaw-importer",
  "author": "Jeramy Wenserit <jeramy@digidip.net>",
  "license": "MIT",
  "devDependencies": {
    "@oclif/tslint": "^3.1.1",
    "@types/deepmerge": "^2.2.0",
    "@types/iconv-lite": "^0.0.1",
    "@types/jest": "^24.0.18",
    "@types/lodash": "^4.14.138",
    "@types/node": "^12.7.5",
    "jest": "^24.9.0",
    "ts-jest": "^24.1.0",
    "ts-node": "^8.4.1",
    "tslint": "^5.20.0",
    "typescript": "^3.6.3"
  },
  "dependencies": {
    "@oclif/command": "^1.5.19",
    "@oclif/config": "^1.13.3",
    "@oclif/parser": "^3.8.4",
    "@oclif/plugin-autocomplete": "^0.1.4",
    "@oclif/plugin-help": "^2.2.1",
    "deepmerge": "^4.0.0",
    "generator-webapp": "^4.0.0-6",
    "globby": "^10.0.1",
    "iconv-lite": "^0.5.0",
    "lodash": "^4.17.15",
    "tslib": "^1.10.0"
  },
  "main": "lib/index.js",
  "bin": {
    "tapjaw": "./bin/tapjaw"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "oclif": {
    "commands": "./lib/commands",
    "bin": "tapjaw",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-autocomplete"
    ]
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "scripts": {
    "postpack": "rm .oclif.manifest.json",
    "prepack": "rm -rf lib && node_modules/typescript/bin/tsc && oclif-dev manifest",
    "lint": "node_modules/tslint/bin/tslint --project .",
    "test": "node node_modules/jest/bin/jest.js"
  },
  "types": "lib/index.d.ts"
}
