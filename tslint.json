{
    "defaultSeverity": "error",
    "extends": "@oclif/tslint",
    "jsRules": {},
    "rules": {
        "semicolon": [true, "always", "ignore-bound-class-methods"],
        "indent": [true, "spaces", 4],
        "ter-indent": [true, 4, { "MemberExpression": 1 }],
        "object-curly-spacing": [true, "always"],
        "ordered-imports": false
    },
    "rulesDirectory": []
}
